{
  "name": "inline-color-annotator",
  "displayName": "Inline Color Annotator",
  "description": "Renders background decorations from sidecar .ann.json files generated by annotate-delimiters.js",
  "version": "0.0.1",
  "publisher": "local",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:inlineColorAnnotator.refresh",
    "onLanguage:*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "inlineColorAnnotator.refresh",
        "title": "Inline Color Annotator: Refresh"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://example.local/inline-color-annotator"
  }
}
